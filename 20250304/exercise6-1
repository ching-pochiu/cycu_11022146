def absolute_value_correct(x):
    if x < 0:
        return -x
    else:
        return x
    
def absolute_value_correct2(x):
    if x < 0:
        return -x
    else:
        return x
    
def is_divisible_idiomatic(x, y):
    return x % y == 0

print(absolute_value_correct(-5))
print(absolute_value_correct2(-5))
prrint(is_divisible_idiomatic(25, 26))